<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.FirebaseLesson"
        tools:targetApi="31">
        <activity
            android:name=".activitys.UsersSearchActivity"
            android:exported="false"
            android:parentActivityName=".activitys.MainActivity"/>
        <activity
            android:name=".activitys.MainActivity"
            android:exported="false" />
        <activity
            android:name=".activitys.ChatActivity"
            android:exported="false"
            android:parentActivityName=".activitys.MainActivity"/>
        <activity
            android:name=".activitys.ActivityMenu"
            android:exported="false"
            android:parentActivityName=".activitys.MainActivity"/>
        <activity
            android:name=".activitys.UpdatePasswordActivity"
            android:exported="false"
            android:parentActivityName=".activitys.MainActivity"/>
        <activity
            android:name=".activitys.ProfileActivity"
            android:exported="false"
            android:parentActivityName=".activitys.MainActivity"/>
        <activity
            android:name=".activitys.ActivitySignUp"
            android:exported="false" />
        <activity
            android:name=".activitys.ActivitySingIn"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>